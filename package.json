    "test-stage": "vite --mode staging"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "axios": "^1.1.3",
    "pinia": "^2.0.23",
    "vant": "^3.6.4",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "sass": "^1.55.0",
    "typescript": "^4.6.4",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.1.0",
    "vue-tsc": "^0.40.4"
  },
  "lint-staged": {
    "*.{js,vue,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
